<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title website_name %}
		</title>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
	</head>

	<body>
		{% include 'layouts/partials/_flash_messages.html.twig' %}

		{% include 'layouts/partials/_navbar.html.twig' %}

		<main role="main"> 
			{% block body %}{% endblock %}
		</main>

		{% include 'layouts/partials/_footer.html.twig' %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</body>
</html>
