<form method="post" action="{{ path('app_delete_video', {'id': video.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cette vidéo ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ video.id) }}">
    <button class="btn btn-primary">Delete</button>
</form>
